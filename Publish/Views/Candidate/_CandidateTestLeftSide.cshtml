@model OnlineTestApp.ViewModel.Company.CandidateTestLeftSideViewModel

<div class="online-test">

    <div class="content-side-left">
        <div class="card-box">
            <div class="icon-card">
                <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                <h2 class="text-white">Total No. of Questions</h2>
            </div>
            <div class="content-card">
                <p id="total-ques-count" total-ques="@Model.TotalQuestions">@Model.TotalQuestions</p>
            </div>
        </div>
        <div class="card-box">
            <div class="icon-card">
                <i class="fa fa-check-square" aria-hidden="true"></i>
                <h2 class="text-white">Attempted Questions</h2>
            </div>
            <div class="content-card">
                <p id="attempted-ques-count" total-attempted="@Model.AttemptedQuestions">@Model.AttemptedQuestions</p>
            </div>
        </div>
        <div class="card-box">
            <div class="icon-card">
                <i class="fa fa-hourglass-half" aria-hidden="true"></i>
                <h2 class="text-white">Pending Questions</h2>
            </div>
            <div class="content-card">
                <p id="pending-ques-count" total-pending="@Model.PendingQuestions">
                    @Model.PendingQuestions
                </p>
            </div>
        </div>

        <div class="card-box">
            <div class="icon-card">
                <i class="fa fa-hourglass-half" aria-hidden="true"></i>
                <h2 class="text-white">Skipped Questions</h2>
            </div>
            <div class="content-card">
                <p id="skipped-ques-count" total-skipped="@Model.SkippedQuestions">
                    @Model.SkippedQuestions
                </p>
            </div>
        </div>
    </div>
</div>