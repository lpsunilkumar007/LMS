<div id="time-tab" class="invite-test-tabs d-none">
    <h3> 3. Select Time Duration </h3>

    <div class="row tim-tab">
        <div class="col-xl-3 col-lg-4 col-md-4 col-sm-4 col-6">
            <a onclick="checkTestDurationRadio('testDuration-option1')" class="time-dur">
                <i class="fa fa-hourglass-half" aria-hidden="true"></i> <span>45 <br>min</span>
                <input type="radio" id="testDuration-option1" name="testDuration" class="custom-control-input " value="45">
            </a>
        </div>
        <div class="col-xl-3 col-lg-4 col-md-4 col-sm-4 col-6">
            <a onclick="checkTestDurationRadio('testDuration-option2')" class="time-dur selected-duration">
                <i class="fa fa-hourglass-half" aria-hidden="true"></i> <span>60 <br>min</span>
                <input type="radio" id="testDuration-option2" name="testDuration" class="custom-control-input " value="60" checked>
            </a>
        </div>
        <div class="col-xl-3 col-lg-4 col-md-4 col-sm-4 col-6">
            <a onclick="checkTestDurationRadio('testDuration-option3')" class="time-dur">
                <i class="fa fa-hourglass-half" aria-hidden="true"></i> <span>90 <br>min</span>
                <input type="radio" id="testDuration-option3" name="testDuration" class="custom-control-input " value="90">
            </a>
        </div>
