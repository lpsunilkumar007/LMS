@model OnlineTestApp.ViewModel.Test.AddNewTestViewModel

<div class="tab-border">
    <h5 id="company-left-panel-text">Experience Level</h5>
    <p>Lorem Ipsum has been the industry's standard dummy text ever since the 500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book.</p>
</div>
<div class="form-group">
    @Html.ListBoxFor(m => m.QuestionTechnology, new SelectList(Model.LstTestTechnology, "LookUpDomainValueId", "LookUpDomainValueText"), "Please Select Technology")
    @Html.ValidationMessageFor(m => m.QuestionTechnology)
</div>
<div class="form-group">
    @Html.DropDownListFor(m => m.QuestionLevel, new SelectList(Model.LstTestLevel, "LookUpDomainValueId", "LookUpDomainValueText"), "Please Select Level")
    @Html.ValidationMessageFor(m => m.QuestionLevel)
</div>
<div class="form-group">
    @Html.TextBoxFor(m => m.TestDetails.TotalTime, new { placeholder = "Total Time" })
    @Html.ValidationMessageFor(m => m.TestDetails.TotalTime)
</div>

<div class="form-group">
    <button class="btn btn-light" type="button" onclick="GetStep2Data()">Proceed</button>
</div>

